@page
@model TokenModel
@{
    ViewData["Title"] = "Token";
    ViewData["ActivePage"] = ManageNavPages.Tokens;
}

<h3>@ViewData["Title"]</h3>
<partial name="_StatusMessage" for="StatusMessage" />
<div class="col-12">
    <form id="profile-form" method="post">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-floating form-group">
            <input asp-for="Token" class="form-control" disabled />
            <label asp-for="Token" class="form-label"></label>
        </div>
        <button id="update-profile-button" type="submit" class="w-100 btn btn-lg btn-dark" >@ViewData["GenerateStatus"]</button>
    </form>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
